{
    "name": "myschoolmanagement/bindto",
    "description": "Simple way to bind Request to DTO/Commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Giulio De Donato",
            "email": "liuggio@gmail.com"
        },
        {
            "name": "Simone Di Maulo",
            "email": "toretto460@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/validator": "^4.2|^5.0",
        "liuggio/filler-dto": "^0.1.0",
        "doctrine/common": "^2.6",
        "psr/http-message": "^1.0"
    },
    "suggest": {
        "symfony/property-access": "Required to use ConvertingObjectMapper"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7|~5.0|~6.0|~7.0|~8.0",
        "symfony/http-foundation": "^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": { "Bindto\\": "src"}
    },
    "autoload-dev": {
        "psr-4": { "Bindto\\": "tests" }
    }
}
